{
  "name": "rescate-animal-voluntariado",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "restart-project": "npx rimraf node_modules .next .cache package-lock.json && npm install && npm run dev",
    "import-mock-users": "node scripts/create_users.js",
    "test": "playwright test",
    "test:sequential": "playwright test --workers=1",
    "test:login": "playwright test --project=login-tests --no-deps",
    "test:roles": "playwright test --project=role-assignment-tests --no-deps",
    "test:history": "playwright test --project=history-view-tests --no-deps",
    "test:user-history": "playwright test --project=user-history-tests --no-deps",
    "test:sequence": "playwright test --project=login-tests --project=role-assignment-tests --project=history-view-tests --project=user-history-tests",
    "test:all-sequential": "npm run test:login && npm run test:roles && npm run test:history && npm run test:user-history",
    "test:ui": "playwright test --ui",
    "test:chrome": "playwright test --project=chrome",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.17.1",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-date-pickers": "^8.3.0",
    "@reduxjs/toolkit": "^2.7.0",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^3.6.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.3.0",
    "next": "^15.3.1",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "playwright": "^1.52.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}