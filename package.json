{
  "name": "rescate-animal-voluntariado",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "restart-project": "npx rimraf node_modules .next .cache package-lock.json && npm install && npm run dev",
    "import-mock-users": "node scripts/create_users.js",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:sequential": "playwright test --workers=1",
    "test:e2e:login": "playwright test --project=login-tests --no-deps",
    "test:e2e:roles": "playwright test --project=role-assignment-tests --no-deps",
    "test:e2e:history": "playwright test --project=history-view-tests --no-deps",
    "test:e2e:user-history": "playwright test --project=user-history-tests --no-deps",
    "test:e2e:user-management": "playwright test --project=user-management-tests --no-deps",
    "test:e2e:sequence": "playwright test --project=login-tests --project=role-assignment-tests --project=history-view-tests",
    "test:e2e:all-sequential": "npm run test:e2e:login && npm run test:e2e:roles && npm run test:e2e:history && npm run test:e2e:user-history",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chrome": "playwright test --project=chrome",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test": "npm run test:unit && npm run test:e2e",
    "test:all": "npm run test:unit:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.17.1",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-date-pickers": "^8.3.0",
    "@reduxjs/toolkit": "^2.7.0",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^3.6.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.3.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.52.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}