{
  "name": "rescate-animal-voluntariado",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "restart-project": "npx rimraf node_modules .next .cache package-lock.json && npm install && npm run dev",
    "import-mock-users": "node scripts/create_users.js",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e": "node tests/scripts/run-tests.js",
    "test:e2e:sequential": "node tests/scripts/run-tests.js --workers=1",
    "test:e2e:login": "node tests/scripts/run-tests.js --project=login-tests --no-deps",
    "test:e2e:roles-assignment": "node tests/scripts/run-tests.js --project=role-assignment-tests --no-deps",
    "test:e2e:history": "node tests/scripts/run-tests.js --project=history-view-tests --no-deps",
    "test:e2e:user-history": "node tests/scripts/run-tests.js --project=user-history-tests --no-deps",
    "test:e2e:admin": "node tests/scripts/run-tests.js --project=admin-features-tests --no-deps",
    "test:e2e:sequence": "node tests/scripts/run-tests.js --project=login-tests --project=role-assignment-tests --project=history-view-tests",
    "test:e2e:all-sequential": "npm run test:e2e:login && npm run test:e2e:roles-assignment && npm run test:e2e:history && npm run test:e2e:user-history && npm run test:e2e:admin",
    "test:e2e:ui": "node tests/scripts/run-tests.js --ui",
    "test:setup-env": "node -e \"require('./tests/helpers/setup-test-environment').setupTestEnvironment({requireUsers: true, requireShifts: true})\"",
    "test:cleanup-env": "node -e \"require('./tests/helpers/setup-test-environment').cleanupTestEnvironment()\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:all": "npm run test:unit:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.17.1",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-date-pickers": "^8.3.0",
    "@reduxjs/toolkit": "^2.7.0",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^3.6.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.3.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
